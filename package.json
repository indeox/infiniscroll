{
  "name": "infiniscroll",
  "version": "1.0.0",
  "scripts": {
    "start": "budo src/render.js --live -- -s render -t [ babelify --presets [ es2015 stage-1 ] ]",
    "build": "browserify src/main.js -s InfiniScroll -t [ babelify --presets [ es2015 stage-1 ] ] -o infiniscroll.js",
    "test": "babel-tape-runner 'src/__tests__/**/*.js'",
    "test:watch": "watchify src/__tests__/render.test.js -o test_bundle.js -v"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ]
  },
  "dependencies": {
    "array-diff": "0.0.1",
    "fastdom": "^1.0.1",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "budo": "^8.3.0",
    "faucet": "0.0.1",
    "tape": "^4.6.0",
    "tape-run": "^2.1.4",
    "watchify": "^3.7.0"
  }
}
