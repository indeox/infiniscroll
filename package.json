{
  "name": "infiniscroll",
  "version": "1.0.0",
  "scripts": {
    "start": "budo src/render.js --live -- --debug -s render -t [ babelify ] -t [ stringify ]",
    "test": "browserify src/__tests__/*.js --debug -t [ babelify ] -t [ stringify ] | tape-run",
    "test:watch": "watchify src/__tests__/*.js --debug -t [ babelify ] -t [ stringify ] -o test_bundle.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ]
  },
  "dependencies": {
    "array-diff": "0.0.1",
    "fastdom": "^1.0.1",
    "if-expression": "^1.0.0",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "budo": "^8.3.0",
    "faucet": "0.0.1",
    "stringify": "^5.1.0",
    "tape": "^4.6.0",
    "tape-run": "^2.1.4",
    "watchify": "^3.7.0"
  }
}
